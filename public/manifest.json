{
  "manifest_version": 3,
  "name": "FocoJá",
  "version": "1.0",
  "description": "Extensão com To-do, Pomodoro e Notas para foco e produtividade.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo.png",
